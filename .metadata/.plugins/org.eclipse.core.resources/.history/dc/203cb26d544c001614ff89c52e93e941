#include <iostream>
using namespace std;

int main() {

	int value = 5;

	switch(value) {
	case 4:
		cout << "value is : " << value << endl;
		break;
	case 5:
			cout << "value is : " << value << endl;
			break;
	case 6:
			cout << "value is : " << value << endl;
			break;
	default:
		cout << "unrecognized value" << endl;
	}
	return 0;
}
