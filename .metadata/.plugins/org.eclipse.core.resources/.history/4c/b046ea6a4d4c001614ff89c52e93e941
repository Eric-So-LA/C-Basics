#include <iostream>
using namespace std;

int main() {

	int values[3];

	values[0] = 88;
	values[1] == 23;
	values[2] == 29;
	cout << values[0] << endl;
	cout << values[1] << endl;
	cout << values[2] << endl;

	return 0;
}
