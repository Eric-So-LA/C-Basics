#include <iostream>
using namespace std;

int main() {

	int values[3];

	values[0] = 88;
	values[1] = 23;
	values[2] = 29;
	cout << values[0] << endl;
	cout << values[1] << endl;
	cout << values[2] << endl;
	for (int i = 0; i < 3; i++){
		cout << values[i] << endl;
	}

	double numbers[4] = {4.5, 2.3, 7.2, 8.1};

	return 0;
}
